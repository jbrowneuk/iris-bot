{
  "name": "iris-bot",
  "version": "0.0.1",
  "description": "Discord chatbot",
  "scripts": {
    "prebuild": "node src/scripts/prebuild.js",
    "build": "tsc",
    "test": "jasmine-ts",
    "coverage": "nyc npm test",
    "develop": "npm run prebuild && ts-node-dev --project tsconfig.json ./src/index.ts",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "prettier --config ./.prettierrc --write ./src/**/*.ts",
    "pr-check": "npm run build && npm run test && npm run lint",
    "quickstart": "npm i && npm run build && npm run start",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbrowneuk/iris-bot.git"
  },
  "keywords": [
    "discord"
  ],
  "author": "Jason Browne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbrowneuk/iris-bot/issues"
  },
  "homepage": "https://github.com/jbrowneuk/iris-bot#readme",
  "dependencies": {
    "discord.js": "^12.3.1",
    "node-fetch": "^2.6.1",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.14",
    "@types/node": "^12.12.62",
    "@types/node-fetch": "^2.5.7",
    "@types/sqlite3": "^3.1.6",
    "@types/ws": "^7.2.6",
    "jasmine": "^3.6.1",
    "jasmine-ts": "^0.3.0",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.62",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.9.7"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/!(*.spec).ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ]
  }
}
