{
  "name": "iris-bot",
  "version": "0.3.0",
  "description": "Discord chatbot",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "prebuild": "node src/scripts/prebuild.js",
    "build": "tsc",
    "test": "node spec/runner.js",
    "coverage": "nyc npm test",
    "develop": "npm run prebuild && ts-node --project tsconfig.json ./src/index.ts",
    "lint": "eslint */**/*.ts",
    "lint:fix": "eslint */**/*.ts --fix",
    "pr-check": "npm run build && npm run test && npm run lint",
    "quickstart": "npm i && npm run build && npm run start",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbrowneuk/iris-bot.git"
  },
  "keywords": [
    "discord"
  ],
  "author": "Jason Browne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbrowneuk/iris-bot/issues"
  },
  "homepage": "https://github.com/jbrowneuk/iris-bot#readme",
  "dependencies": {
    "@vscode/sqlite3": "^5.0.7",
    "axios": "^0.26.0",
    "bufferutil": "^4.0.6",
    "discord.js": "^13.6.0",
    "http-status-codes": "^2.2.0",
    "minecraft-server-util": "^3.5.0",
    "node-cleanup": "^2.1.2",
    "utf-8-validate": "^5.0.8"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.4",
    "@types/node": "^16.0.0",
    "@types/node-cleanup": "^2.1.1",
    "@types/sqlite3": "^3.1.6",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "jasmine": "^3.6.4",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "typemoq": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/!(*.spec).ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ]
  }
}
