{
  "name": "iris-bot",
  "version": "0.1.0",
  "description": "Discord chatbot",
  "scripts": {
    "prebuild": "node src/scripts/prebuild.js",
    "build": "tsc",
    "test": "jasmine-ts",
    "coverage": "nyc npm test",
    "develop": "npm run prebuild && ts-node --project tsconfig.json ./src/index.ts",
    "lint": "eslint */**/*.ts",
    "lint:fix": "eslint */**/*.ts --fix",
    "pr-check": "npm run build && npm run test && npm run lint",
    "quickstart": "npm i && npm run build && npm run start",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbrowneuk/iris-bot.git"
  },
  "keywords": [
    "discord"
  ],
  "author": "Jason Browne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbrowneuk/iris-bot/issues"
  },
  "homepage": "https://github.com/jbrowneuk/iris-bot#readme",
  "dependencies": {
    "discord.js": "12.5.1",
    "node-cleanup": "^2.1.2",
    "node-fetch": "2.6.1",
    "sqlite3": "5.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.7",
    "@types/node-cleanup": "^2.1.1",
    "@types/node-fetch": "^2.5.7",
    "@types/sqlite3": "^3.1.6",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "jasmine": "^3.6.3",
    "jasmine-ts": "^0.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^7.0.0",
    "typemoq": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/!(*.spec).ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ]
  }
}
