{
  "name": "iris-bot",
  "version": "0.0.1",
  "description": "Discord chatbot",
  "scripts": {
    "build": "tsc",
    "test": "jasmine-ts",
    "coverage": "nyc npm test",
    "watch": "nodemon",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "prettier --config ./.prettierrc --write ./src/**/*.ts",
    "pr-check": "npm run build && npm run test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbrowneuk/iris-bot.git"
  },
  "keywords": [
    "discord"
  ],
  "author": "Jason Browne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbrowneuk/iris-bot/issues"
  },
  "homepage": "https://github.com/jbrowneuk/iris-bot#readme",
  "dependencies": {
    "discord.js": "11.3.2",
    "inversify": "4.13.0",
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "9.6.6",
    "jasmine-ts": "0.2.1",
    "nodemon": "1.17.3",
    "nyc": "11.7.1",
    "prettier": "1.12.1",
    "ts-node": "6.0.0",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.8.0",
    "tslint-consistent-codestyle": "1.13.0",
    "tslint-eslint-rules": "5.1.0",
    "typemoq": "2.1.0",
    "typescript": "2.8.3"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/!(*.spec.*).ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ]
  }
}
