{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "types": ["node", "discord.js"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "outDir": "dist",
    "rootDir": "src"
  },
  "exclude": ["**/*.spec.ts", "**/*.specdata.ts", "spec/**"]
}
